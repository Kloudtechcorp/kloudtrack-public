# Company API Configuration (Server-side only - DO NOT add NEXT_PUBLIC_ prefix)
COMPANY_API_BASE_URL=https://api.yourcompany.com
COMPANY_API_TOKEN=your_secret_api_token_here

# Mapbox API Key (Client-side - needs NEXT_PUBLIC_ prefix)
NEXT_PUBLIC_MAPBOX_API_KEY=pk.your_mapbox_token_here

# Polling Configuration (Client-side)
NEXT_PUBLIC_POLLING_INTERVAL=300000

# Optional: Caching Configuration (Server-side)
ENABLE_API_CACHE=true
CACHE_TTL_SECONDS=60